{
  "name": "consulta-detran-electron",
  "productName": "Consulta Detran",
  "version": "1.0.0",
  "description": "Aplicação para automatizar processo de consulta a dados de CNH no Detran.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MateusVT/Electron-Consulta-Detran.git"
  },
  "bin": "./lib/setup.js",
  "main": ".webpack/main",
  "build": {
    "appId": "com.torrescorp.ConsultaDetran",
    "productName": "Consulta Detran",
    "copyright": "Copyright © 2021 Mateus Torres",
    "win": {
      "publisherName": "Torres Corp",
      "icon": "./public/logo.png"
    },
    "publish": [
      "github"
    ],
    "files": [
      "build",
      "public",
      "./build/**/*",
      "./dist/**/*",
      "./node_modules/**/*",
      "./public/**/*",
      "*.js",
      "**/*",
      "public/logo.*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "scripts": {
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always",
    "tag": "node ./config/tag.js",
    "auto-changelog": "auto-changelog --hide-credit --breaking-pattern \"breaking:\"",
    "gen-changelog": "npm run auto-changelog && git add CHANGELOG.md && git commit -m \"doc: change log\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publishOnly": "electron-forge publish",
    "postpublish": "npm-run-all -s tag gen-changelog"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "starter",
    "boilerplate"
  ],
  "author": "Mateus Torres",
  "license": "MIT",
  "config": {
    "repository": {
      "owner": "MateusVT",
      "name": "Electron-Consulta-Detran"
    },
    "prerelease": true,
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-ts-react"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@hot-loader/react-dom": "^17.0.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/axios": "^0.14.0",
    "@types/mongodb": "^3.6.10",
    "@types/node-fetch": "^2.5.8",
    "@types/tough-cookie": "^4.0.0",
    "anticaptcha": "^2.0.3",
    "axios": "^0.21.1",
    "base64-js": "^1.5.1",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "electron-chromedriver": "^12.0.0",
    "electron-log": "^4.3.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^7.0.2",
    "electron-updater": "^4.3.8",
    "electron-winstaller": "^5.0.0",
    "form-data": "^4.0.0",
    "material-table": "^1.69.2",
    "moment": "^2.29.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.12",
    "react-keyboard-event-handler": "^1.5.4",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "unzipper": "^0.10.11",
    "webdriver": "^7.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/cors": "^2.8.10",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "auto-changelog": "^2.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.1",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "jquery": "^3.6.0",
    "node-loader": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1"
  },
  "browser": {
    "http": false
  }
}
